  },
  "dependencies": {
    "@nuxtjs/axios": "5.0.0",
    "@nuxtjs/dotenv": "1.3.0",
    "cross-env": "5.2.0",
    "express": "4.16.3",
    "nuxt": "2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "5.0.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "4.0.0",
    "nodemon": "1.11.0",
    "prettier": "1.14.3"
  }
}
