{
  "name": "simple-vue",
  "version": "0.0.3",
  "description": "Sample Vue.js Project",
  "author": "Recep Köseoğlu",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
